{
  "name": "vite-plugin-static-md",
  "version": "0.5.3",
  "author": "Andrew Chang-DeWitt",
  "license": "MIT",
  "homepage": "https://github.com/andrew-chang-dewitt/vite-plugin-static-md#readme",
  "bugs": {
    "url": "https://github.com/andrew-chang-dewitt/vite-plugin-static-md/issues"
  },
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "tsc-watch --onSuccess \"npm run --prefix ./example/ dev -- --host 0.0.0.0\"",
    "check": "tsc --noEmit",
    "test": "vitest",
    "test:once": "vitest run",
    "coverage": "vitest run --coverage",
    "prebuild": "rm -rf dist && npm run test:once",
    "build": "tsc",
    "watch": "tsc -w",
    "prerelease": "npm run build",
    "release": "commit-and-tag-version",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "prettier": {
    "semi": false
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "vite": "5.4.0 || ^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "jsdom": "^26.1.0",
    "marked": "^15.0.7",
    "marked-directive": "^1.0.7",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "commit-and-tag-version": "^12.5.2",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "ts-todo": "^2.0.0",
    "tsc-watch": "^7.1.1",
    "typescript": "^5.9.2",
    "vitest": "^3.0.4"
  }
}
